        var msg = document.getElementById("filomsg");

        if (fcant > cantidad || ((fcant + fcantn) > cantidad)) {

            msg.innerHTML = " Cantidad Incorrecta"
            msg.classList.add('text-danger');


        } else {
            msg.innerHTML = ""
